on member join guild:
	set {_m} to event-member
	wait 80 tick
	if {_m} is set:
		set {_id} to {_m}.getUser().getId()
		set {_nick} to getMcNick({_id})
		if {_nick} contains "-":
			set {_nick} to "%event-member.getUser()%"
		set {_now} to "%now%"
		execute "INSERT INTO `akcje` (id,nick,now) VALUES('%unsafe {_id}%', '%unsafe {_nick}%', '%unsafe {_now}%')" in {bot::sql}
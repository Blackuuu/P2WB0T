function getMcNick(u: text, null: boolean=true) :: text:
	set {_g} to getGuild()
	set {_member} to getUser({_u}) in {_g}
	if {_member} is not set:
		if {_null} is true:
			return "- (Nie mial zmienionego nicku)"
		else:
			return "%{_member}%"
	set {_nick} to discord nickname of {_member}
	if {_nick} contains "[":
		set {_nick} to last element of ({_nick} split at " ")
		return {_nick}
	if {_nick} != discord name of {_member}:
		return {_nick}
	else:
		return "- (Nie mial zmienionego nicku)"
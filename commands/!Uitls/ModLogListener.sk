function modLog(karany: text, adm: text, typ: text, powod: text, czas: text="null"):
	set {_guild} to guild with id {bot::guild}
	set {_b} to {bot::name} parsed as bot

	set {_karany} to getUser({_karany})
	set {_adm} to getUser({_adm})
	set {_kurwa} to {_guild}.getMemberById({_adm}.getId())

	set {_nickname} to discord nickname of ({_karany} in {_guild})
	set {_embed::karany} to "%getMention({_karany}.getId())%(%getName({_karany}.getId())%)"
	if {_nickname} != "%{_karany}%":
		set {_embed::karany} to "%{_embed::karany}% [%{_nickname}%]"

	create embed:
		set the colour of the embed to color of getColor({_kurwa})
		set the timestamp of embed to now
		set the footer of embed to footer with text {_typ} and icon "%avatar of {_b}%"
	
		set the author info of the embed to author named "Author name (Can point to URL)" with no url and icon "link"
	
		add field named "Osoba karana:" with value {_embed::karany} to embed
		add field named "Administrator:" with value "%getMention({_adm}.getId())%(%getName({_adm}.getId())%) [%discord nickname of {_kurwa}%]" to embed
		add field named "Pow√≥d:" with value getPowod({_powod}) to embed
		if {_czas} != "null":
			add field named "Mija o:" with value {_czas} to embed
	
		set the thumbnail of embed to "%avatar of {_karany}%"
	send last created embed to channel with id getConfig("Config.modlogi") with {bot::name}
	addAkcje({_karany}.getId(), {_adm}.getId(), {_typ}, {_powod}, {_czas})
